{
    "name": "U2Pplayer",
    "plugin": "plugin.video.themoviedb.helper",
    "priority": 100,
    "assert": {
        "play_movie": [
            "title",
            "year"
        ],
        "play_episode": [
            "showname",
            "season",
            "episode"
        ],
        "search_movie": [
            "title"
        ],
        "search_episode": [
            "showname"
        ]
    },
    "play_movie": [
        "plugin://plugin.video.sendtokodiU2P/?action=detailM&u2p={tmdb}",
        {
            "position": "2"
        },
        {
            "dialog": "Auto"
        }
    ],
    "play_episode": [
        "plugin://plugin.video.sendtokodiU2P/?action=tmdbSerie&u2p={tmdb}&saison={season}",
        {
            "position": "{episode}"
        }
    ],
    "search_movie": [
        "plugin://plugin.video.sendtokodiU2P/?action=mediasHKFilms&famille=Search&offset=0&limit=500",
        {
            "keyboard": "{title}"
        }
    ],
    "search_episode": [
        "plugin://plugin.video.sendtokodiU2P/?action=mediasHKSeries&famille=Search&offset=0&limit=500",
        {
            "keyboard": "{showname}"
        }
    ],
    "is_resolvable": "true",
    "make_playlist": "upnext",
    "fallback": {
        "play_movie": "U2Pplayer.json search_movie",
        "play_episode": "U2Pplayer.json search_episode",
        "search_movie": "youtube.json play_movie",
        "search_episode": "youtube.json play_episode"
    }
}